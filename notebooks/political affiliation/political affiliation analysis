    "democrats = merged.loc[merged[\"Party\"]== \"Democratic\" , :]\n",
    "democrats\n",
    "\n",
    "dems_sorted = democrats.sort_values(\n",
    "by=\"Death Rate per 100000\",\n",
    "ascending=True)\n",
    "dems_sorted\n",
    "\n",
    "dems_renamed = dems_sorted.rename(columns={\"State/Territory\" : \"Dem State/Territory\"})\n",
    "dems = dems_renamed.rename(columns={\"Death Rate per 100000\" : \"Dem Death Rate per 100000\"})\n",
    "dems\n",
    "del dems[\"Party\"]\n",
    "dems"
   ]
